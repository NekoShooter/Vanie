{
  "name": "vanie",
  "version": "0.0.3",
  "description": "Vanie es una librería diseñada para el desarrollo de interfaces de usuario interactivas en el front-end. Con un enfoque en la creación de ventanas arrastrables, Vanie ofrece una experiencia de usuario familiar, inspirada en los sistemas operativos más emblemáticos: Windows, macOS y Linux.",
  "files": [
    "README.md",
    "index.js",
    "src"
  ],
  "scripts": {
    "demo": "webpack --config ./Demo/webpack.config.js --mode=production && servor ./Demo index.html 8080",
    "live-dev": "webpack --watch --config ./Demo/webpack.config.js --mode=development",
    "live-server": "servor ./Demo index.html 8080 --reload",
    "info": "echo \" https://github.com/NekoShooter/vanie.git#readme \" && exit 1"
  },
  "keywords": [
    "draggable",
    "window",
    "modals",
    "resizable",
    "UI",
    "ventana",
    "re-escalable",
    "frontend"
  ],
  "author": "Neko Shooter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NekoShooter/vanie.git"
  },
  "bugs": {
    "url": "https://github.com/NekoShooter/vanie/issues"
  },
  "homepage": "https://github.com/NekoShooter/vanie#readme",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "servor": "4.0.2",
    "webpack": "5.91.0",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {
    "nauty": "1.0.0"
  }
}
